{
  "version": 2,
  "headers": [
    {
      "source": "/api/chat",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://labuenamesarestaurant.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ],
  "functions": {
    "api/chat.js": {
      "maxDuration": 10
    }
  }
}
